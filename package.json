{
  "name": "milaan",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "cross-env APP_ENV=local expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start:dev": "cross-env APP_ENV=development expo start",
    "start:stg": "cross-env APP_ENV=staging expo start",
    "start:prd": "cross-env APP_ENV=production expo start",
    "build:dev": "cross-env APP_ENV=development && cd android && gradlew assembleRelease",
    "build:stg": "cross-env APP_ENV=staging && cd android && gradlew assembleRelease",
    "build:prd": "cross-env APP_ENV=production && cd android && gradlew assembleRelease"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "cross-env": "^7.0.3",
    "expo": "~51.0.28",
    "expo-auth-session": "~5.5.2",
    "expo-crypto": "~13.0.2",
    "expo-secure-store": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "expo-web-browser": "~13.0.3",
    "formik": "^2.4.6",
    "jwt-decode": "^4.0.0",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-paper": "^5.12.5",
    "react-native-safe-area-context": "^4.11.1",
    "react-native-vector-icons": "^10.2.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
